{
	"presets": [
		["@babel/preset-react"],
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current",
					"esmodules": true
				}
			}
		]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-syntax-dynamic-import",
		[
			"babel-plugin-dotenv",
			{
				"replacedModuleName": "babel-dotenv"
			}
		]
	]
}
